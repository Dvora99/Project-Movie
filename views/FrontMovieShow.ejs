    <style>
        .main_movie {
            padding: 35px 0;
            display: flex;
            justify-content: flex-start;
            border-bottom: 1px solid hsla(0, 0%, 6%, .07);
            user-select: none;
            margin: 0 24px;
        }

        .main_movie .movie_name {
            width: 387px;
            margin-right: 35px;
            -webkit-tap-highlight-color: transparent;
            color: #000;
            font-size: 25px;
        }

        .main_movie .movie_show {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            grid-auto-rows: auto;
            align-items: baseline;
            justify-content: center;
            grid-row-gap: 25px;
            grid-column-gap: 16px;
        }

        .movie_show .single_show {
            position: relative;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 10px;
            color: #506d85;
            font-weight: 300;
        }

        .movie_show .single_show .show_time {
            width: 131px;
            height: 68px;
            padding: 12px 8px;
            border-radius: 8px;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 16px;
            font-weight: 700;
            line-height: 24px;
            border: 1px solid #000;
            box-sizing: border-box;
            letter-spacing: -.08px;
        }

        .movie_show .single_show .show_time .seat_price {
            font-size: 10px;
            color: #8ba6c1;
            font-weight: 500;
            line-height: 16px
        }

        .containers1 {
            max-width: 1140px;
            margin: 0 auto;
            padding: 0 15px;
        }

        .carousel-caption {
            position: absolute;
            top: 20%;
            text-align: left;
            padding: 30px 20px;
            bottom: 8rem;
            left: 35%;
            width: 60%;
            color: #fff;
        }

        :root {
            --size: 150px;
            --radius: calc(var(--size) * 20 / 100);
        }

        body {
            margin: 0;
        }

        main {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            height: 100vh;
            width: 100%;
            margin: auto;
        }

        .image-card {
            width: var(--size);
            height: var(--size);
            border-radius: var(--radius);
            position: relative;
            background-size: cover;
            margin: calc(var(--size) * 20 / 100);
        }
    </style>

</head>

